{
  "openapi": "3.0.3",
  "info": {
    "version": "1.0",
    "title": "API",
    "description": "API"
  },
  "servers": [
    {
      "url": "<%= ENV["API_DOCUMENTATION_DEVELOPMENT_HOST"] %>/api",
      "description": "Development server"
    }
  ],
  "tags": [
    { "name": "Interface languages" },
    { "name": "Content languages" }
  ],
  "components": {
    "responses": {
      "unauthorized": {
        "description": "Unauthorized access",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "error"
              ],
              "properties": {
                "error": {
                  "type": "string",
                  "description": "Error message",
                  "examples": ["Unauthorized"]
                }
              }
            }
          }
        }
      },
      "forbidden": {
        "description": "Access forbidden",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "error"
              ],
              "properties": {
                "error": {
                  "type": "string",
                  "description": "Error message",
                  "examples": ["Forbidden"]
                }
              }
            }
          }
        }
      },
      "not_found": {
        "description": "Not found",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "error"
              ],
              "properties": {
                "error": {
                  "type": "string",
                  "description": "Error message",
                  "examples": ["Not Found"]
                }
              }
            }
          }
        }
      }
    }
  },
  "paths": {
    "/interface_languages.json": {
      "get": {
        "tags": ["Interface languages"],
        "summary": "Interface languages",
        "description": "Sorted by id asc \n\n Not paginated",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": <%= schema("interface_languages_controller/index/200") %>
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/unauthorized"
          },
          "403": {
            "$ref": "#/components/responses/forbidden"
          }
        }
      }
    },
    "/interface_languages/:id.json": {
      "get": {
        "tags": ["Interface languages"],
        "summary": "Interface language",
        "description": "",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": <%= schema("interface_languages_controller/show/200") %>
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/unauthorized"
          },
          "403": {
            "$ref": "#/components/responses/forbidden"
          },
          "404": {
            "$ref": "#/components/responses/not_found"
          }
        }
      }
    },
    "/content_languages.json": {
      "get": {
        "tags": ["Content languages"],
        "summary": "Content languages",
        "description": "Sorted by id asc \n\n Not paginated",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": <%= schema("content_languages_controller/index/200") %>
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/unauthorized"
          },
          "403": {
            "$ref": "#/components/responses/forbidden"
          }
        }
      }
    },
    "/content_languages/:id.json": {
      "get": {
        "tags": ["Content languages"],
        "summary": "Content language",
        "description": "",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": <%= schema("content_languages_controller/show/200") %>
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/unauthorized"
          },
          "403": {
            "$ref": "#/components/responses/forbidden"
          },
          "404": {
            "$ref": "#/components/responses/not_found"
          }
        }
      }
    }
  }
}