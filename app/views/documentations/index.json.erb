        "schema": {
          "type": "integer",
          "default": 50,
          "minimum": 1,
          "maximum": 50
        }
      }
    },
    "responses": {
      "no_content": {
        "description": "No content",
        "content": {
          "schema": {
            "type": "integer"
          }
        }
      },
      "unauthorized": {
        "description": "Unauthorized access",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "error"
              ],
              "properties": {
                "error": {
                  "type": "string",
                  "description": "Error message",
                  "examples": ["Unauthorized"]
                }
              }
            }
          }
        }
      },
      "forbidden": {
        "description": "Access forbidden",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "error"
              ],
