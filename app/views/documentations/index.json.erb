          "application/json": {
            "schema": {
              "type": "object",
              "required": [
                "error"
              ],
              "properties": {
                "error": {
                  "type": "string",
                  "description": "Error message",
                  "examples": ["Unauthorized"]
                }
              }
            }
          }
        }
      },
      "forbidden": {
        "description": "Access forbidden",
