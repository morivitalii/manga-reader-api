      "post": {
        "tags": ["Titles"],
        "summary": "Create title",
        "description": "",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "description": "Title",
                    "examples": ["Made in Abyss"]
                  },
                  "description": {
                    "type": "string",
                    "description": "Description",
                    "examples": ["Awesome manga"]
                  },
                  "publication_status": {
                    "type": "string",
                    "description": "Publication status",
                    "enum": ["ongoing", "completed", "cancelled"],
                    "examples": ["ongoing"]
                  },
                  "original_content_language_id": {
                    "type": "integer",
                    "description": "Original content language",
                    "examples": [1]
                  },
                  "writer_ids": {
                    "type": "array",
                    "description": "Writers",
                    "items": {
                      "type": "integer",
                      "examples": [1]
                    }
                  },
                  "painter_ids": {
                    "type": "array",
                    "description": "Painters",
                    "items": {
                      "type": "integer",
                      "examples": [1]
                    }
                  },
                  "genre_ids": {
                    "type": "array",
                    "description": "Genres",
                    "items": {
                      "type": "integer",
                      "examples": [1]
                    }
                  },
                  "format_ids": {
                    "type": "array",
                    "description": "Formats",
                    "items": {
                      "type": "integer",
                      "examples": [1]
                    }
                  },
                  "demographic_ids": {
